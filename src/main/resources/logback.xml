<configuration debug="false">

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - BLA %msg%n</pattern>
    </encoder>
  </appender>

  <!-- Do not print debug output from swagger-parser -->
  <logger name="io.swagger.v3" level="INFO"/>

  <root level="debug">
    <appender-ref ref="STDOUT"/>
  </root>
  
</configuration>